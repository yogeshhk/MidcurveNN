%%% New definitions of scalable "big"'s!
\def\big#1{{\hbox{$\left#1\vbox to 0.85em{}\right.\n@space$}}}
\def\Big#1{{\hbox{$\left#1\vbox to 1.15em{}\right.\n@space$}}}
\def\bigg#1{{\hbox{$\left#1\vbox to 1.45em{}\right.\n@space$}}}
\def\Bigg#1{{\hbox{$\left#1\vbox to 1.75em{}\right.\n@space$}}}
\def\Biggg#1{{\hbox{$\left#1\vbox to 2.05em{}\right.\n@space$}}}
\def\Bigggg#1{{\hbox{$\left#1\vbox to 2.35em{}\right.\n@space$}}}
\def\Biggggg#1{{\hbox{$\left#1\vbox to 2.65em{}\right.\n@space$}}}
