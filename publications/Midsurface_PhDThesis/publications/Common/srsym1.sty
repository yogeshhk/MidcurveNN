\def\RegularMathSpacing{%
  \thinmuskip=3mu
  \medmuskip=4mu plus 2mu minus 4mu
  \thickmuskip=5mu plus 5mu}%
\def\PSMathSpacing{%
  \thinmuskip=2.5mu
  \medmuskip=2.5mu plus 1mu minus 1mu
  \thickmuskip=4mu plus 1.5mu minus 1mu}%
\def\MyMathSpacing#1#2#3#4#5#6#7{%
  \thinmuskip=#1mu
  \medmuskip=#2mu plus #3mu minus #4mu
  \thickmuskip=#5mu plus #6mu minus #7mu}%
% \MyMathSpacing{3}{4}{2}{4}{5}{5}{0} % <-- \RegularMathSpacing
% \MyMathSpacing{2.5}{2.5}{1}{1}{4}{1.5}{1} % <-- \PSMathSpacing
\PSMathSpacing
